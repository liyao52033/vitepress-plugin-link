{
  "name": "vitepress-plugin-link",
  "type": "module",
  "version": "1.4.2",
  "private": false,
  "description": "扫描 markdown 文档，生成永久链接",
  "keywords": [
    "vitepress",
    "vite",
    "plugin",
    "permalink"
  ],
  "license": "MIT",
  "homepage": "https://github.com/liyao52033/vitepress-plugin-link.git",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liyao52033/vitepress-plugin-link.git"
  },
  "bugs": {
    "url": "https://github.com/liyao52033/vitepress-plugin-link/issues"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./usePermalink": {
      "types": "./dist/usePermalink.d.ts",
      "import": "./dist/usePermalink.mjs",
      "require": "./dist/usePermalink.cjs"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.mjs",
      "require": "./dist/*.cjs"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ],
      "usePermalink": [
        "./dist/usePermalink.d.ts"
      ]
    }
  },
  "scripts": {
    "stub": "unbuild --stub",
    "build": "unbuild"
  },
  "dependencies": {
    "gray-matter": "^4.0.3",
    "picocolors": "^1.1.1",
    "url-parse": "^1.5.10"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "@types/url-parse": "^1.4.11",
    "typescript": "^5.8.2",
    "unbuild": "^3.2.0",
    "vite": "^6.0.7",
    "vitepress": "^1.6.3"
  },
  "peerDependencies": {
    "vite": "^6.0.7",
    "vitepress": "^1.6.3"
  }
}
